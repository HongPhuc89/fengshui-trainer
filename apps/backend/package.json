{
  "name": "@quiz-game/backend",
  "version": "0.0.1",
  "description": "Quiz Game Backend API - NestJS",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "node -e \"require('fs').cpSync('../../config', 'dist/config', {recursive: true})\"",
    "format": "prettier --write \"src/**/*.{ts,json}\" \"*.{ts,json,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\" \"*.{ts,json,js}\"",
    "migration:run": "ts-node -r tsconfig-paths/register src/scripts/run-migrations.ts",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "create:admin": "ts-node -r tsconfig-paths/register scripts/create-admin.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@langchain/core": "^1.1.0",
    "@langchain/textsplitters": "^1.0.1",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@supabase/supabase-js": "^2.86.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.12",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.12",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.2",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.2",
    "@types/papaparse": "^5.5.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2"
  }
}
