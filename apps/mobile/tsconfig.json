{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/modules/*": ["modules/*"],
      "@/hooks/*": ["hooks/*"],
      "@/utils/*": ["utils/*"],
      "@/components/*": ["components/*"],
      "@/services/*": ["services/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "global.d.ts"]
}
